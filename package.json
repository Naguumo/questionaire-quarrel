{
  "name": "questionaire-quarrel",
  "version": "1.0.0",
  "description": "Family Feud on the web",
  "repository": "https://github.com/Naguumo/questionaire-quarrel.git",
  "author": "Ishaan <ishbharal@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "socket.io": "^3.1.0",
    "socket.io-client": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "parcel-bundler": "^1.12.4"
  },
  "scripts": {
    "dev": "yarn run dev:web && yarn run dev:server",
    "dev:web": "parcel public/index.html",
    "dev:server": "cd server && nodemon server.js",
    "start": "cd server && node server.js",
    "build": "parcel build public/index.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
